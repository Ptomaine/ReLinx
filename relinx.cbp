<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="relinx" />
		<Option pch_mode="2" />
		<Option compiler="clang" />
		<Build>
			<Target title="Debug-Clang">
				<Option output="bin/Debug/relinx" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="clang" />
				<Compiler>
					<Add option="-g" />
					<Add option="-target x86_64-w64-mingw32" />
				</Compiler>
				<Linker>
					<Add option="-target x86_64-w64-mingw32" />
				</Linker>
			</Target>
			<Target title="Release-Clang">
				<Option output="bin/Release/relinx" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="clang" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wnon-virtual-dtor" />
					<Add option="-Wbind-to-temporary-copy" />
					<Add option="-Wambiguous-member-template" />
					<Add option="-pedantic-errors" />
					<Add option="-pedantic" />
					<Add option="-Wall" />
					<Add option="-target x86_64-w64-mingw32" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-target x86_64-w64-mingw32" />
				</Linker>
			</Target>
			<Target title="Release-GCC">
				<Option output="bin/Release/relinx" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-std=c++1z" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-static" />
				</Linker>
			</Target>
			<Target title="Debug-GCC">
				<Option output="bin/Debug/relinx" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-std=c++1z" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-static" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++14" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="main.cpp" />
		<Unit filename="relinx/relinx.hxx">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<DoxyBlocks>
				<comment_style block="1" line="2" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
